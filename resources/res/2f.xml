<?xml version="1.0" encoding="utf-8"?>
<com.atakmap.android.uastool.tasks.route.edit.GimbalPitchEdit xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="horizontal" android:id="@+id/pointedit_gimbal_pitch_layout" android:paddingTop="18dp" android:layout_width="match_parent" android:layout_height="wrap_content">
